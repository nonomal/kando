    "icon": "award_star",
    "iconTheme": "material-symbols-rounded",
    "children": [
      {
        "type": "submenu",
        "name": "example-menu.apps.submenu",
        "icon": "apps",
        "iconTheme": "material-symbols-rounded",
        "children": [
          {
            "type": "command",
            "data": {
              "command": "open -a Safari"
            },
            "name": "example-menu.apps.safari",
            "icon": "safari",
            "iconTheme": "simple-icons"
          },
          {
            "type": "command",
            "data": {
              "command": "open -a Mail"
            },
            "name": "example-menu.apps.email",
            "icon": "mail",
            "iconTheme": "material-symbols-rounded"

          {
            "type": "command",
            "data": {
              "command": "osascript -e 'tell application \"Music\" to previous track'"
            },
            "name": "example-menu.audio.previous-track",
            "icon": "skip_previous",
            "iconTheme": "material-symbols-rounded"
          }
        ]
      },
      {
